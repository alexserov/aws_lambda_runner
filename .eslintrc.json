{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "ignorePatterns": "build",
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "off",
        "import/extensions": "off",
        "import/no-unresolved": "off",
        "no-plusplus": "off",
        "no-continue": "off",
        "class-methods-use-this": "off",
        "max-len": "off",
        "indent":["warn", 4]
    }
}
